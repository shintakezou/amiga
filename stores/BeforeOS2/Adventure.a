		lea	north(pc),a0
		tst.b	RM_NFlag(a1)
		beq	vsud
		bsr	_print
		bsr	spazio
vsud		lea	south(pc),a0
		tst.b	RM_SFlag(a1)
		beq	vest
		bsr	_print
		bsr	spazio
vest		lea	east(pc),a0
		tst.b	RM_EFlag(a1)
		beq	vovest
		bsr	_print
		bsr	spazio
vovest		lea	west(pc),a0
		tst.b	RM_WFlag(a1)
		beq.s	vfine
		bsr	_print
vfine		lea	accapo_m(pc),a0
		bsr	_print

		lea	inv_m(pc),a0
		bsr	_print
		lea	str_man0(pc),a0
		move.l	MA_CurrentRoom(a0),a1
		move.l	RM_FirstObj(a1),a1
		cmpa.l	#0,a1
		beq	noinroom

obj_search	move.l	OBJ_Name(a1),a0
		bsr	_print
		bsr	spazio
		move.l	OBJ_Next(a1),d0
		beq.s	no_other_obj
		move.l	d0,a1
		bra.s	obj_search

noinroom	lea	nobjrm_m(pc),a0
		bra	_print

nobjrm_m	dc.b	'NOTHING',10,10,0
		even

no_other_obj	lea	str_man0(pc),a0
		move.l	MA_CurrentRoom(a0),a1
		lea	control(pc),a2
next_sman	move.l	(a2)+,d0
		beq	no_man
		move.l	d0,a3
		cmp.l	a0,a3
		beq	no_man
		move.l	MA_CurrentRoom(a3),a0
		cmp.l	a0,a1
		bne	no_man
		move.l	MA_Name(a3),a0
		bsr	_print
		bsr	spazio
		bra	next_sman
no_man		lea	accapo_m(pc),a0
		bra	_print


accapo_m	dc.b	10,10,0
		even
direc_m		dc.b	'You can go: ',0
		even
inv_m		dc.b	'You can see: ',0
		even


_dothem		lea	control(pc),a0
next_ctrl	move.l	(a0),a1
